{
    "name": "customizations",
    "displayName": "Customizations",
    "description": "My personal customizations for VSCode",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "customizations.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.7.13",
        "@types/vscode": "^1.60.0",
        "typescript": "^4.4.2"
    }
}
